{
  "name": "alldrit.ca",
  "version": "1.0.0",
  "description": "Alldrit.ca site",
  "author": "Devin Alldrit <devin@aldrit.ca>",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint . && prettier --check .",
    "lint:diff": "git diff --name-only --cached --diff-filter=ACMRTUXB | grep .js | xargs -r eslint",
    "prettier:diff": "git diff --name-only --cached --diff-filter=ACMRTUXB | grep .js | xargs -r prettier --check",
    "fmt": "prettier --write .",
    "dev": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby build && aws s3 sync public/ s3://alldrit --delete --exclude .DS_Store",
    "test": "jest"
  },
  "license": "0BSD",
  "dependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "gatsby": "^6.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.2.0"
  },
  "devDependencies": {
    "@testing-library/react": "^15.0.0",
    "babel-jest": "^30.0.0",
    "babel-preset-gatsby": "^4.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-react": "^7.33.2",
    "gatsby-plugin-sitemap": "^7.0.0",
    "husky": "^9.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.0.0",
    "jest-environment-jsdom": "^30.0.0",
    "prettier": "^3.0.0"
  }
}
